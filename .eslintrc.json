{
  "extends": ["plugin:prettier/recommended", "next/core-web-vitals"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "printWidth": 100,
        "semi": false,
        "singleQuote": false,
        "tabWidth": 2,
        "bracketSpacing": true,
        "trailingComma": "es5",
        "useTabs": false
      }
    ]
  }
}
